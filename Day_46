 public static void main(String[] args) {
            int count=0;
            int arr[]={1,4,5,7,2};
            for(int i=0;i<arr.length;i++){
                for(int j=0;j<10;j++){
                    if(arr[i]==j*j)
                    {
                        count++;
                        break;
                    }
    
                }
            }
            System.out.println(count);
        }
